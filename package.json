{
  "name": "mms-schema-design-toolkit",
  "version": "2.0.0",
  "description": "Professional MongoDB schema design toolkit for healthcare systems - Interactive validation, testing, and optimization tools for member management schemas",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:schema": "node tests/schema-validator.js",
    "test:queries": "node tests/query-tester.js",
    "lint": "eslint src/ tests/ --ext .js,.mjs",
    "format": "prettier --write .",
    "build": "npm run lint && npm run test",
    "schema:validate": "node tools/schema-validator.js",
    "queries:benchmark": "node tools/query-benchmark.js",
    "docs:generate": "jsdoc -c jsdoc.config.json"
  },
  "keywords": [
    "mongodb",
    "schema-design",
    "healthcare",
    "member-management",
    "database-optimization",
    "validation",
    "indexing",
    "aggregation",
    "medical-records"
  ],
  "author": "jgschmitz",
  "license": "MIT",
  "dependencies": {
    "mongodb": "^6.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "joi": "^17.10.0",
    "ajv": "^8.12.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "jsdoc": "^4.0.2",
    "@types/jest": "^29.5.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jgschmitz/MMS-Schema-Design.git"
  },
  "bugs": {
    "url": "https://github.com/jgschmitz/MMS-Schema-Design/issues"
  },
  "homepage": "https://github.com/jgschmitz/MMS-Schema-Design#readme"
}